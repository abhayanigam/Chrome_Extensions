{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "helloWorld.html",
      "default_icon": "hello_extensions.png"
    },
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        }
      }
    }
  }